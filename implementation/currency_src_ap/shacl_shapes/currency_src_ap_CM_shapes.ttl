curr-shape:euvoc-Currency a sh:NodeShape ;
    rdfs:label "Currency" ;
    rdfs:comment "The Currency authority table is a controlled vocabulary that lists concepts associated with currencies and currency subunits. The concepts included are correlated with the ISO 4217 international standard. The Currency authority table is updated based on the stakeholders’ needs." ;
    rdfs:isDefinedBy curr-shape:curr-shape ;
    sh:property curr-shape:euvoc-Currency-euvoc-decimalPlace,
        curr-shape:euvoc-Currency-skos-inScheme ;
    sh:targetClass euvoc:Currency .

curr-shape:euvoc-Currency-euvoc-decimalPlace a sh:PropertyShape ;
    rdfs:isDefinedBy curr-shape:curr-shape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:name "Decimal place" ;
    sh:path euvoc:decimalPlace .

curr-shape:euvoc-Currency-skos-inScheme a sh:PropertyShape ;
    sh:class at-voc:currency ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "In scheme" ;
    sh:path skos:inScheme .

