cat2-shape:euvoc-CorporateBody a sh:NodeShape ;
    rdfs:label "Corporate body" ;
    rdfs:comment "The Corporate body authority table is a controlled vocabulary which provides the varying names of EU corporate bodies for a given time span. Some other – international and national – corporate bodies are also included as far as they are used in the various systems in the Publications Office. The traceability of the chronological modifications is ensured when possible." ;
    rdfs:isDefinedBy cat2-shape:cat2-shape ;
    sh:property cat2-shape:euvoc-CorporateBody-euvoc-hasPrimaryContactPoint,
        cat2-shape:euvoc-CorporateBody-euvoc-legallyBasedOnTreaty,
        cat2-shape:euvoc-CorporateBody-euvoc-order,
        cat2-shape:euvoc-CorporateBody-foaf-homepage,
        cat2-shape:euvoc-CorporateBody-skos-inScheme ;
    sh:targetClass euvoc:CorporateBody .

cat2-shape:euvoc-Treaty a sh:NodeShape ;
    rdfs:label "Treaty" ;
    rdfs:comment "The Treaty authority table (AT) is a controlled vocabulary listing the different EU treaties, including treaties and protocols, accession treaties, Schengen Convention and Charter of Fundamental Rights. The Treaty AT is part of the Core Metadata used in the exchange between the institutions involved in the legal decision-making process and the Publications Office of the European Union. It is also used by EUR-Lex. It provides an authority code, the start-use date of the concept and labels in all official EU languages." ;
    rdfs:isDefinedBy cat2-shape:cat2-shape ;
    sh:property cat2-shape:euvoc-Treaty-skos-inScheme ;
    sh:targetClass euvoc:Treaty .

cat2-shape:vcard-ContactPoint a sh:NodeShape ;
    rdfs:label "Contact point" ;
    rdfs:comment "Information (e.g. e-mail address, telephone number) of a Person or department through which the user can get in touch with." ;
    rdfs:isDefinedBy cat2-shape:cat2-shape ;
    sh:property cat2-shape:vcard-ContactPoint-dct-description,
        cat2-shape:vcard-ContactPoint-vcard-hasEmail,
        cat2-shape:vcard-ContactPoint-vcard-telephone ;
    sh:targetClass vcard:ContactPoint .

cat2-shape:euvoc-CorporateBody-euvoc-hasPrimaryContactPoint a sh:PropertyShape ;
    sh:class vcard:ContactPoint ;
    sh:name "Has primary contact point" ;
    sh:path euvoc:hasPrimaryContactPoint ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://publications.europa.eu/ontology/euvoc#hasPrimaryContactPoint> ?that . ?that <http://publications.europa.eu/ontology/euvoc#hasPrimaryContactPoint> ?this .}" ] .

cat2-shape:euvoc-CorporateBody-euvoc-legallyBasedOnTreaty a sh:PropertyShape ;
    sh:class euvoc:Treaty ;
    sh:name "Legally based on treaty" ;
    sh:path euvoc:legallyBasedOnTreaty ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://publications.europa.eu/ontology/euvoc#legallyBasedOnTreaty> ?that . ?that <http://publications.europa.eu/ontology/euvoc#legallyBasedOnTreaty> ?this .}" ] .

cat2-shape:euvoc-CorporateBody-euvoc-order a sh:PropertyShape ;
    rdfs:isDefinedBy cat2-shape:cat2-shape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:name "Order" ;
    sh:path euvoc:order .

cat2-shape:euvoc-CorporateBody-foaf-homepage a sh:PropertyShape ;
    rdfs:isDefinedBy cat2-shape:cat2-shape ;
    sh:datatype xsd:anyURI ;
    sh:description "The main web page used by the instance of the concept." ;
    sh:maxCount 1 ;
    sh:name "Homepage" ;
    sh:path foaf:homepage .

cat2-shape:euvoc-CorporateBody-skos-inScheme a sh:PropertyShape ;
    sh:class at-voc:corporate-body ;
    sh:description "A concept scheme in which the concept is included. A concept may be a member of more than one concept scheme." ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "In scheme" ;
    sh:path skos:inScheme .

cat2-shape:euvoc-Treaty-skos-inScheme a sh:PropertyShape ;
    sh:class at-voc:treaty ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "In scheme" ;
    sh:path skos:inScheme .

cat2-shape:vcard-ContactPoint-dct-description a sh:PropertyShape ;
    rdfs:isDefinedBy cat2-shape:cat2-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:name "Description" ;
    sh:path dcterms:description .

cat2-shape:vcard-ContactPoint-vcard-hasEmail a sh:PropertyShape ;
    rdfs:isDefinedBy cat2-shape:cat2-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:maxCount 1 ;
    sh:name "Has email" ;
    sh:path vcard:hasEmail .

cat2-shape:vcard-ContactPoint-vcard-telephone a sh:PropertyShape ;
    rdfs:isDefinedBy cat2-shape:cat2-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:name "Telephone" ;
    sh:path vcard:telephone .

