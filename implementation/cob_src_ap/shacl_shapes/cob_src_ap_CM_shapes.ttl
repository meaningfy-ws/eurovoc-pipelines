cob-shape:euvoc-CorporateBody a sh:NodeShape ;
    rdfs:label "Corporate body" ;
    rdfs:comment "The Corporate body authority table is a controlled vocabulary which provides the varying names of EU corporate bodies for a given time span. Some other – international and national – corporate bodies are also included as far as they are used in the various systems in the Publications Office. The traceability of the chronological modifications is ensured when possible." ;
    rdfs:isDefinedBy cob-shape:cob-shape ;
    sh:property cob-shape:euvoc-CorporateBody-euvoc-hasPrimaryContactPoint,
        cob-shape:euvoc-CorporateBody-euvoc-order,
        cob-shape:euvoc-CorporateBody-foaf-homepage,
        cob-shape:euvoc-CorporateBody-skos-inScheme ;
    sh:targetClass euvoc:CorporateBody .

cob-shape:vcard-ContactPoint a sh:NodeShape ;
    rdfs:label "Contact point" ;
    rdfs:comment "Information (e.g. e-mail address, telephone number) of a Person or department through which the user can get in touch with." ;
    rdfs:isDefinedBy cob-shape:cob-shape ;
    sh:property cob-shape:vcard-ContactPoint-dct-description,
        cob-shape:vcard-ContactPoint-vcard-hasEmail,
        cob-shape:vcard-ContactPoint-vcard-telephone ;
    sh:targetClass vcard:ContactPoint .

cob-shape:euvoc-CorporateBody-euvoc-hasPrimaryContactPoint a sh:PropertyShape ;
    sh:class vcard:ContactPoint ;
    sh:description "It indicate the primary means of contact for an entity (e.g., an organization or corporate body), linking that entity to a specific contact point" ;
    sh:name "Has primary contact point" ;
    sh:path euvoc:hasPrimaryContactPoint ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://publications.europa.eu/ontology/euvoc#hasPrimaryContactPoint> ?that . ?that <http://publications.europa.eu/ontology/euvoc#hasPrimaryContactPoint> ?this .}" ] .

cob-shape:euvoc-CorporateBody-euvoc-order a sh:PropertyShape ;
    rdfs:isDefinedBy cob-shape:cob-shape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:name "Order" ;
    sh:path euvoc:order .

cob-shape:euvoc-CorporateBody-foaf-homepage a sh:PropertyShape ;
    rdfs:isDefinedBy cob-shape:cob-shape ;
    sh:datatype xsd:anyURI ;
    sh:description "The main web page used by the instance of the concept." ;
    sh:maxCount 1 ;
    sh:name "Homepage" ;
    sh:path foaf:homepage .

cob-shape:euvoc-CorporateBody-skos-inScheme a sh:PropertyShape ;
    sh:class at-voc:corporate-body ;
    sh:description "A concept scheme in which the concept is included. A concept may be a member of more than one concept scheme." ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "In scheme" ;
    sh:path skos:inScheme .

cob-shape:vcard-ContactPoint-dct-description a sh:PropertyShape ;
    rdfs:isDefinedBy cob-shape:cob-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:name "Description" ;
    sh:path dcterms:description .

cob-shape:vcard-ContactPoint-vcard-hasEmail a sh:PropertyShape ;
    rdfs:isDefinedBy cob-shape:cob-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:maxCount 1 ;
    sh:name "Has email" ;
    sh:path vcard:hasEmail .

cob-shape:vcard-ContactPoint-vcard-telephone a sh:PropertyShape ;
    rdfs:isDefinedBy cob-shape:cob-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:name "Telephone" ;
    sh:path vcard:telephone .

