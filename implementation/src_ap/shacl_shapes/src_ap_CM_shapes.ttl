    sh:maxCount 1 ;
    sh:name "Version info" ;
    sh:path owl:versionInfo .

src-shape:skos-ConceptScheme-skos-hasTopConcept a sh:PropertyShape ;
    sh:class skos:Concept ;
    sh:name "Has top concept" ;
