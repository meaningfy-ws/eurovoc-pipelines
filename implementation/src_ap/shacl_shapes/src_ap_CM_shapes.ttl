@prefix : <http://data.europa.eu/a4g/ontology#> .
@prefix at-voc: <http://publications.europa.eu/resource/authority/> .
@prefix cccev: <http://data.europa.eu/m8g/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix euvoc: <http://publications.europa.eu/ontology/euvoc#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix skosxl: <http://www.w3.org/2008/05/skos-xl#> .
@prefix src-shape: <http://data.europa.eu/a4g/data-shape#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

src-shape:euvoc-XlNotation a sh:NodeShape ;
    rdfs:label "Xl notation" ;
    rdfs:comment "A notation is a string of characters used to uniquely identify a concept within a specified context. Like the skosxl:Label class reifies SKOS label statements, XlNotation reifies SKOS notation statements. This class permits, if needed, to maintain the historical view of the values and add additional provenance descriptions." ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:property src-shape:euvoc-XlNotation-dct-created,
        src-shape:euvoc-XlNotation-dct-modified,
        src-shape:euvoc-XlNotation-euvoc-endDate,
        src-shape:euvoc-XlNotation-euvoc-startDate,
        src-shape:euvoc-XlNotation-owl-deprecated,
        src-shape:euvoc-XlNotation-rdf-value ;
    sh:targetClass euvoc:XlNotation .

src-shape:euvoc-XlNote a sh:NodeShape ;
    rdfs:label "Xl note" ;
    rdfs:comment "Like the skosxl:Label class reifies SKOS label statements, XlNote reifies SKOS note statements (i.e. skos:editorialNote, skos:example, skos:historyNote, skos:definition, skos:scopeNote and skos:changeNote). This class permits, if needed, to maintain the historical view of the values and add additional provenance descriptions." ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:property src-shape:euvoc-XlNote-dct-created,
        src-shape:euvoc-XlNote-dct-creator,
        src-shape:euvoc-XlNote-dct-modified,
        src-shape:euvoc-XlNote-euvoc-endDate,
        src-shape:euvoc-XlNote-euvoc-startDate,
        src-shape:euvoc-XlNote-owl-deprecated,
        src-shape:euvoc-XlNote-rdf-value ;
    sh:targetClass euvoc:XlNote .

src-shape:skos-Concept a sh:NodeShape ;
    rdfs:label "Concept" ;
    rdfs:comment "A concept is a unit of thought. The semantic content of a concept can be re-expressed by a combination of other and different concepts, which may vary from one language or culture to another. Concepts exist in the mind as abstract entities that are independent of the terms used to label them." ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:property src-shape:skos-Concept-dct-created,
        src-shape:skos-Concept-dct-dateAccepted,
        src-shape:skos-Concept-dct-dateSubmitted,
        src-shape:skos-Concept-dct-isReplacedBy,
        src-shape:skos-Concept-dct-modified,
        src-shape:skos-Concept-dct-replaces,
        src-shape:skos-Concept-euvoc-endDate,
        src-shape:skos-Concept-euvoc-startDate,
        src-shape:skos-Concept-euvoc-xlChangeNote,
        src-shape:skos-Concept-euvoc-xlDefinition,
        src-shape:skos-Concept-euvoc-xlHistoryNote,
        src-shape:skos-Concept-euvoc-xlNotation,
        src-shape:skos-Concept-euvoc-xlNote,
        src-shape:skos-Concept-owl-deprecated,
        src-shape:skos-Concept-owl-versionInfo,
        src-shape:skos-Concept-skos-broader,
        src-shape:skos-Concept-skos-exactMatch,
        src-shape:skos-Concept-skos-inScheme,
        src-shape:skos-Concept-skos-narrower,
        src-shape:skos-Concept-skos-topConceptOf,
        src-shape:skos-Concept-skosxl-altLabel,
        src-shape:skos-Concept-skosxl-hiddenLabel,
        src-shape:skos-Concept-skosxl-prefLabel ;
    sh:targetClass skos:Concept .

src-shape:skos-ConceptScheme a sh:NodeShape ;
    rdfs:label "Concept scheme" ;
    rdfs:comment "A concept schema is a set of concepts, which may include statements about the semantic relationships between those concepts. Thesauri, classification schemes, subject heading lists, taxonomies, terminologies, glossaries, and other types of controlled vocabularies are examples of concept schemas." ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:property src-shape:skos-ConceptScheme-dct-created,
        src-shape:skos-ConceptScheme-dct-identifier,
        src-shape:skos-ConceptScheme-dct-modified,
        src-shape:skos-ConceptScheme-dct-title,
        src-shape:skos-ConceptScheme-euvoc-endDate,
        src-shape:skos-ConceptScheme-euvoc-startDate,
        src-shape:skos-ConceptScheme-owl-deprecated,
        src-shape:skos-ConceptScheme-owl-versionInfo,
        src-shape:skos-ConceptScheme-skos-hasTopConcept ;
    sh:targetClass skos:ConceptScheme .

src-shape:skosxl-Label a sh:NodeShape ;
    rdfs:label "Label" ;
    rdfs:comment "The class skosxl:Label is a special class of lexical entities. An instance of the class skosxl:Label is a resource and may be named with a URI. An instance of the class skosxl:Label has a single literal form. This literal form is an RDF plain literal (which is a string of UNICODE characters and an optional language tag [<xref scope=\"external\" href=\"https://www.w3.org/TR/rdf-concepts/\"><foreign otherprops=\"#0000ff\"><u>RDF-CONCEPTS</u></foreign></xref>]). The property skosxl:literalForm is used to give the literal form of an skosxl:Label. If two instances of the class skosxl:Label have the same literal form, they are not necessarily the same resource." ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:property src-shape:skosxl-Label-dct-created,
        src-shape:skosxl-Label-dct-modified,
        src-shape:skosxl-Label-euvoc-endDate,
        src-shape:skosxl-Label-euvoc-startDate,
        src-shape:skosxl-Label-owl-deprecated,
        src-shape:skosxl-Label-skosxl-literalForm ;
    sh:targetClass skosxl:Label .

src-shape:euvoc-XlNotation-dct-created a sh:PropertyShape ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:datatype xsd:dateTime ;
    sh:description "Date of creation of the resource." ;
    sh:maxCount 1 ;
    sh:name "Created" ;
    sh:path dcterms:created .

src-shape:euvoc-XlNotation-dct-modified a sh:PropertyShape ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:datatype xsd:dateTime ;
    sh:description "Date of modification of the resource." ;
    sh:maxCount 1 ;
    sh:name "Modified" ;
    sh:path dcterms:modified .

src-shape:euvoc-XlNotation-euvoc-endDate a sh:PropertyShape ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:datatype xsd:date ;
    sh:description "End of the validity period. If a resource has an end date then it must be marked as deprecated." ;
    sh:maxCount 1 ;
    sh:name "End date" ;
    sh:path euvoc:endDate .

src-shape:euvoc-XlNotation-euvoc-startDate a sh:PropertyShape ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:datatype xsd:date ;
    sh:description "Beginning of the validity period." ;
    sh:maxCount 1 ;
    sh:name "Start date" ;
    sh:path euvoc:startDate .

src-shape:euvoc-XlNotation-owl-deprecated a sh:PropertyShape ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:datatype xsd:boolean ;
    sh:description "States whether the resource is current or deprecated. By deprecating a resource, it means that it should not be used in new documents. Deprecation is a feature commonly used in versioning software to indicate that a particular feature is preserved for backward-compatibility purposes, but may be phased out in the future." ;
    sh:maxCount 1 ;
    sh:name "Deprecated" ;
    sh:path owl:deprecated .

src-shape:euvoc-XlNotation-rdf-value a sh:PropertyShape ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:datatype rdf:langString ;
    sh:description "The literal form of the note." ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "Value" ;
    sh:path rdf:value .

src-shape:euvoc-XlNote-dct-created a sh:PropertyShape ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:datatype xsd:dateTime ;
    sh:maxCount 1 ;
    sh:name "Created" ;
    sh:path dcterms:created .

src-shape:euvoc-XlNote-dct-creator a sh:PropertyShape ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:name "Creator" ;
    sh:path dcterms:creator .

src-shape:euvoc-XlNote-dct-modified a sh:PropertyShape ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:datatype xsd:dateTime ;
    sh:maxCount 1 ;
    sh:name "Modified" ;
    sh:path dcterms:modified .

src-shape:euvoc-XlNote-euvoc-endDate a sh:PropertyShape ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:description "End of the validity period. If a resource has an end date then it must be marked as deprecated." ;
    sh:maxCount 1 ;
    sh:name "End date" ;
    sh:path euvoc:endDate .

src-shape:euvoc-XlNote-euvoc-startDate a sh:PropertyShape ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:datatype xsd:date ;
    sh:description "Beginning of the validity period." ;
    sh:maxCount 1 ;
    sh:name "Start date" ;
    sh:path euvoc:startDate .

src-shape:euvoc-XlNote-owl-deprecated a sh:PropertyShape ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:datatype xsd:boolean ;
    sh:maxCount 1 ;
    sh:name "Deprecated" ;
    sh:path owl:deprecated .

src-shape:euvoc-XlNote-rdf-value a sh:PropertyShape ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:datatype rdf:langString ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "Value" ;
    sh:path rdf:value .

src-shape:skos-Concept-dct-created a sh:PropertyShape ;
    rdfs:isDefinedBy src-shape:src-shape ;
    sh:datatype xsd:dateTime ;
    sh:description "Date of creation of the resource." ;
    sh:maxCount 1 ;
    sh:name "Created" ;
    sh:path dcterms:created .

