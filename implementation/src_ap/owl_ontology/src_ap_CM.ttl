    dcterms:title "common SRC_AP Ontology  - core"@en ;
    vann:preferredNamespacePrefix "epo" ;
    vann:preferredNamespaceUri "http://data.europa.eu/a4g/ontology#" ;
    rdfs:comment "This version is automatically generated from src_ap_CM.xml on 2023-11-03" ;
