    dcterms:publisher "http://publications.europa.eu/resource/authority/corporate-body/PUBL" ;
    dcterms:title "common SRC_AP Ontology - core restrictions"@en ;
    vann:preferredNamespacePrefix "epo" ;
    vann:preferredNamespaceUri "http://data.europa.eu/a4g/ontology#" ;
    rdfs:comment """This version is automatically generated from technical_CM.xml on 
                2023-12-01""" ;
    rdfs:seeAlso <https://docs.ted.europa.eu/home/index.html>,
        <https://github.com/OP-TED/ePO/releases>,
        <https://joinup.ec.europa.eu/collection/eprocurement/solution/eprocurement-ontology/about>,
        <https://op.europa.eu/en/web/eu-vocabularies/e-procurement> ;
    owl:imports :tech,
        cccev:,
        <http://data.europa.eu/nuts>,
        <http://lexvo.org/ontology>,
        <http://publications.europa.eu/ontology/euvoc>,
        at-voc:,
        dcterms:,
        <http://www.lexinfo.net/ontology/2.0/lexinfo>,
        <http://www.opengis.net/ont/geosparql>,
        <http://www.opengis.net/ont/sf>,
        <http://www.w3.org/2003/01/geo/wgs84_pos>,
        <http://www.w3.org/2004/02/skos/core>,
        <http://www.w3.org/2006/time>,
        <http://www.w3.org/2006/vcard/ns>,
        <http://www.w3.org/2008/05/skos-xl>,
        <http://www.w3.org/ns/adms>,
        <http://www.w3.org/ns/locn>,
        org:,
        <http://www.w3.org/ns/person>,
        foaf:,
        <https://data.europa.eu/m8g/>,
        <ttp://publications.europa.eu/ontology/authority/> ;
    owl:incompatibleWith "3.1.0" ;
    owl:priorVersion "http://data.europa.eu/a4g/ontology#tech-restriction-3.1.0" ;
    owl:versionIRI :tech-restriction-4.0.0 ;
    owl:versionInfo "4.0.0" .

lexvo:inScript rdfs:domain skosxl:Label ;
    rdfs:range euvoc:Script .

euvoc:context rdfs:domain skos:Concept ;
    rdfs:range euvoc:UseContext .

euvoc:corrStatus rdfs:domain [ a owl:Class ;
            owl:unionOf ( euvoc:XlNote skosxl:Label ) ] ;
    rdfs:range euvoc:CorrectionStatus .

euvoc:countryCode rdfs:domain euvoc:Site ;
    rdfs:range euvoc:Country .

euvoc:hasPrimaryContactPoint rdfs:domain [ a owl:Class ;
            owl:unionOf ( euvoc:Site euvoc:CorporateBody ) ] ;
    rdfs:range vcard:ContactPoint .

euvoc:legallyBasedOnTreaty rdfs:domain euvoc:CorporateBody ;
    rdfs:range euvoc:Treaty .

euvoc:localityCode rdfs:domain vcard:Address ;
    rdfs:range euvoc:Place .

euvoc:status rdfs:domain [ a owl:Class ;
            owl:unionOf ( skosxl:Label skos:Concept ) ] ;
    rdfs:range euvoc:ConceptStatus .

euvoc:usedIn rdfs:domain euvoc:Currency ;
    rdfs:range euvoc:Country .

dcterms:description rdfs:domain vcard:ContactPoint ;
    rdfs:range rdf:PlainLiteral .

lexinfo:gender rdfs:domain skosxl:Label ;
    rdfs:range euvoc:GrammaticalGender .

lexinfo:number rdfs:domain skosxl:Label ;
    rdfs:range euvoc:GrammaticalNumber .

geo:sfWithin rdfs:domain euvoc:Place ;
    rdfs:range euvoc:Country .

wgs1:location rdfs:domain euvoc:Place ;
    rdfs:range wgs1:SpatialThing .

vcard:hasAddress rdfs:domain euvoc:Site ;
    rdfs:range vcard:Address .

vcard:telephone rdfs:domain vcard:ContactPoint ;
    rdfs:range rdf:PlainLiteral .

locn:geometry rdfs:domain euvoc:Site ;
    rdfs:range sf:Polygon .

org:role rdfs:domain euvoc:Place ;
    rdfs:range euvoc:CapitalClassification .

euvoc:postbox rdfs:domain vcard:Address ;
    rdfs:range xsd:string .

at-voc:country a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue at-voc:country ;
            owl:onProperty skos:inScheme ] .

at-voc:treaty a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue at-voc:treaty ;
            owl:onProperty skos:inScheme ] .

dcterms:type a owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( vcard:Address skosxl:Label euvoc:XlNotation ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( euvoc:AddressType euvoc:Label-type euvoc:NotationType ) ] .

geo:asWKT a owl:FunctionalProperty ;
    rdfs:domain sf:Polygon ;
    rdfs:range rdf:PlainLiteral .

geo:lat a owl:FunctionalProperty ;
    rdfs:domain wgs1:SpatialThing ;
    rdfs:range xsd:string .

geo:long a owl:FunctionalProperty ;
    rdfs:domain wgs1:SpatialThing ;
    rdfs:range xsd:string .

rdfs:comment a owl:FunctionalProperty ;
    rdfs:domain wgs1:SpatialThing ;
    rdfs:range rdf:PlainLiteral .

vcard:hasEmail rdfs:domain vcard:ContactPoint ;
    rdfs:range rdf:PlainLiteral .

vcard:locality a owl:FunctionalProperty ;
    rdfs:domain vcard:Address ;
    rdfs:range xsd:string .

vcard:post-office-box rdfs:domain vcard:Address ;
    rdfs:range xsd:string .

vcard:postal-code rdfs:domain vcard:Address ;
    rdfs:range xsd:string .

vcard:street-address rdfs:domain vcard:Address ;
    rdfs:range xsd:string .

dcterms:created rdfs:domain [ a owl:Class ;
            owl:unionOf ( vcard:Address wgs1:SpatialThing ) ] ;
    rdfs:range xsd:dateTime .

dcterms:modified rdfs:domain [ a owl:Class ;
            owl:unionOf ( sf:Polygon vcard:Address ) ] ;
    rdfs:range xsd:dateTime .

skos:inScheme rdfs:domain [ a owl:Class ;
            owl:unionOf ( euvoc:Country euvoc:Treaty ) ] ;
    rdfs:range skos:Concept .

sf:Polygon a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty geo:asWKT ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:modified ] .

vcard:ContactPoint rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty vcard:hasEmail ] .

wgs1:SpatialThing a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty geo:lat ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty geo:long ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:created ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty rdfs:comment ] .

vcard:Address a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty euvoc:postbox ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcterms:created ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty dcterms:type ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty vcard:postal-code ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty vcard:post-office-box ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty vcard:locality ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcterms:modified ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty vcard:street-address ] .

[] a owl:AllDisjointClasses ;
    owl:members ( euvoc:CapitalClassification euvoc:ConceptStatus euvoc:CorrectionStatus euvoc:Country euvoc:Label-type euvoc:NotationType euvoc:Treaty euvoc:UseContext ) .

