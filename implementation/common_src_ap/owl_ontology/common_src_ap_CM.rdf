   <owl:Class rdf:about="http://publications.europa.eu/ontology/euvoc#NotationType">
      <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
   </owl:Class>
   <owl:Class rdf:about="http://publications.europa.eu/ontology/euvoc#Script">
      <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
   </owl:Class>
   <owl:Class rdf:about="http://publications.europa.eu/ontology/euvoc#UseContext">
      <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
   </owl:Class>
   <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#inScheme"/>
   <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#inScheme">
      <skos:prefLabel xml:lang="en">In scheme</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#inScheme">
      <skos:definition xml:lang="en">A Concept Scheme in which the Concept is included. A Concept may be a member of more than one concept scheme. (skos:Concept -&gt; skos:ConceptScheme (+skos:inScheme))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/type"/>
   <rdf:Description rdf:about="http://purl.org/dc/terms/type">
      <skos:prefLabel xml:lang="en">Type</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://purl.org/dc/terms/type">
      <skos:definition xml:lang="en">Specify the context where a specified notation is considered unique. (euvoc:XlNotation -&gt; at-voc:notation-type (+dct:type))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://publications.europa.eu/ontology/euvoc#xlNotation"/>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#xlNotation">
      <skos:prefLabel xml:lang="en">Xl notation</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#xlNotation">
      <skos:definition xml:lang="en">A notation is a string of characters such as "T58.5" or "303.4833" used to uniquely identify a concept within the scope of a given concept scheme or within a specified context. (skos:Concept -&gt; euvoc:XlNotation (+euvoc:xlNotation))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://publications.europa.eu/ontology/euvoc#corrStatus"/>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#corrStatus">
      <skos:prefLabel xml:lang="en">Corr status</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#corrStatus">
      <skos:definition xml:lang="en">A status of proofreading of labels and descriptions. (euvoc:XlNote -&gt; at-voc:correction-status (+euvoc:corrStatus)) A status of proofreading of labels and descriptions. (skosxl:Label -&gt; at-voc:correction-status (+euvoc:corrStatus))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://publications.europa.eu/ontology/euvoc#xlHistoryNote"/>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#xlHistoryNote">
      <skos:prefLabel xml:lang="en">Xl history note</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#xlHistoryNote">
      <skos:definition xml:lang="en">A note about the past state/use/meaning of a concept. (skos:Concept -&gt; euvoc:XlNote (+euvoc:xlHistoryNote))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://publications.europa.eu/ontology/euvoc#xlNote"/>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#xlNote">
      <skos:prefLabel xml:lang="en">Xl note</skos:prefLabel>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://publications.europa.eu/ontology/euvoc#xlChangeNote"/>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#xlChangeNote">
      <skos:prefLabel xml:lang="en">Xl change note</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#xlChangeNote">
      <skos:definition xml:lang="en">A note about a modification to a concept. (skos:Concept -&gt; euvoc:XlNote (+euvoc:xlChangeNote))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://publications.europa.eu/ontology/euvoc#xlDefinition"/>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#xlDefinition">
      <skos:prefLabel xml:lang="en">Xl definition</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#xlDefinition">
      <skos:definition xml:lang="en">A statement or formal explanation of the meaning of a concept. (skos:Concept -&gt; euvoc:XlNote (+euvoc:xlDefinition))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://www.w3.org/2008/05/skos-xl#altLabel"/>
   <rdf:Description rdf:about="http://www.w3.org/2008/05/skos-xl#altLabel">
      <skos:prefLabel xml:lang="en">Alt label</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.w3.org/2008/05/skos-xl#altLabel">
      <skos:definition xml:lang="en">An alternative lexical label for a resource. Acronyms, abbreviations, spelling variants, and irregular plural/singular forms may be included among the alternative labels for a concept. (skos:Concept -&gt; skosxl:Label (+skosxl:altLabel))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://www.w3.org/2008/05/skos-xl#prefLabel"/>
   <rdf:Description rdf:about="http://www.w3.org/2008/05/skos-xl#prefLabel">
      <skos:prefLabel xml:lang="en">Pref label</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.w3.org/2008/05/skos-xl#prefLabel">
      <skos:definition xml:lang="en">The preferred lexical label for a resource, in a given language. No two concepts in the same concept scheme may have the same preferred label in a given language. (skos:Concept -&gt; skosxl:Label (+skosxl:prefLabel))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#exactMatch"/>
   <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#exactMatch">
      <skos:prefLabel xml:lang="en">Exact match</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#exactMatch">
      <skos:definition xml:lang="en">The property skos:exactMatch is used to link two concepts, indicating a high degree of confidence that the concepts can be used interchangeably across a wide range of information retrieval applications. skos:exactMatch is a transitive property, and is a sub-property of skos:closeMatch. (skos:Concept -&gt; skos:Concept (+skos:exactMatch))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#topConceptOf"/>
   <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#topConceptOf">
      <skos:prefLabel xml:lang="en">Top concept of</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#topConceptOf">
      <skos:definition xml:lang="en">A concept scheme in which the concept is included. A concept may be a member of more than one concept scheme. (skos:Concept -&gt; skos:ConceptScheme (+skos:topConceptOf))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/isReplacedBy"/>
   <rdf:Description rdf:about="http://purl.org/dc/terms/isReplacedBy">
      <skos:prefLabel xml:lang="en">Is replaced by</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://purl.org/dc/terms/isReplacedBy">
      <skos:definition xml:lang="en">A related resource that supplants, displaces, or supersedes the described resource. (skos:Concept -&gt; skos:Concept (+dct:isReplacedBy))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#narrower"/>
   <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#narrower">
      <skos:prefLabel xml:lang="en">Narrower</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#narrower">
      <skos:definition xml:lang="en">A concept that is less general in meaning. Narrower concepts are typically rendered as children in a concept hierarchy (tree). (skos:Concept -&gt; skos:Concept (+skos:narrower))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#broader"/>
   <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#broader">
      <skos:prefLabel xml:lang="en">Broader</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#broader">
      <skos:definition xml:lang="en">A concept that is more general in meaning. Broader concepts are typically rendered as parents in a concept hierarchy (tree). (skos:Concept -&gt; skos:Concept (+skos:broader))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://publications.europa.eu/ontology/euvoc#context"/>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#context">
      <skos:prefLabel xml:lang="en">Context</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#context">
      <skos:definition xml:lang="en">It signifies that the concept is applicable or relevant within the context of the identified use context. (skos:Concept -&gt; at-voc:use-context (+euvoc:context))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://www.w3.org/2008/05/skos-xl#hiddenLabel"/>
   <rdf:Description rdf:about="http://www.w3.org/2008/05/skos-xl#hiddenLabel">
      <skos:prefLabel xml:lang="en">Hidden label</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.w3.org/2008/05/skos-xl#hiddenLabel">
      <skos:definition xml:lang="en">A lexical label for a resource that should be hidden when generating visual displays of the resource, but should still be accessible to free text search operations. Mis-spelled terms are normally included as hidden labels. (skos:Concept -&gt; skosxl:Label (+skosxl:hiddenLabel))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/replaces"/>
   <rdf:Description rdf:about="http://purl.org/dc/terms/replaces">
      <skos:prefLabel xml:lang="en">Replaces</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://purl.org/dc/terms/replaces">
      <skos:definition xml:lang="en">A related resource that is supplanted, displaced, or superseded by the described resource. (skos:Concept -&gt; skos:Concept (+dct:replaces))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://publications.europa.eu/ontology/euvoc#status"/>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#status">
      <skos:prefLabel xml:lang="en">Status</skos:prefLabel>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#hasTopConcept"/>
   <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#hasTopConcept">
      <skos:prefLabel xml:lang="en">Has top concept</skos:prefLabel>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#gender"/>
   <rdf:Description rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#gender">
      <skos:prefLabel xml:lang="en">Gender</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#gender">
      <skos:definition xml:lang="en">A grammatical category that indicates grammatical relationships between words in sentences. (skosxl:Label -&gt; at-voc:gramatical-gender (+lexinfo:gender))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://lexvo.org/ontology#inScript"/>
   <rdf:Description rdf:about="http://lexvo.org/ontology#inScript">
      <skos:prefLabel xml:lang="en">In script</skos:prefLabel>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#number"/>
   <rdf:Description rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#number">
      <skos:prefLabel xml:lang="en">Number</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#number">
      <skos:definition xml:lang="en">Grammatical category for the variation in form of nouns, pronouns, and any words agreeing with them, depending on how many persons or things are referred to. (skosxl:Label -&gt; at-voc:grammatical-number (+lexinfo:number))</skos:definition>
   </rdf:Description>
   <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/created"/>
   <rdf:Description rdf:about="http://purl.org/dc/terms/created">
      <skos:prefLabel xml:lang="en">Created</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://purl.org/dc/terms/created">
      <skos:definition xml:lang="en">Date of creation of the resource. (euvoc:XlNotation) Date of creation of the resource. (euvoc:XlNote) Date of creation of the resource. (skos:Concept) Date of creation of the resource. (skos:ConceptScheme) Date of creation of the resource. (skosxl:Label)</skos:definition>
   </rdf:Description>
   <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/modified"/>
   <rdf:Description rdf:about="http://purl.org/dc/terms/modified">
      <skos:prefLabel xml:lang="en">Modified</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://purl.org/dc/terms/modified">
      <skos:definition xml:lang="en">Date of modification of the resource. (euvoc:XlNotation) Date of modification of the resource. (euvoc:XlNote) Date of modification of the resource. (skos:Concept) Date of modification of the resource. (skos:ConceptScheme) Date of modification of the resource. (skosxl:Label)</skos:definition>
   </rdf:Description>
   <owl:DatatypeProperty rdf:about="http://publications.europa.eu/ontology/euvoc#endDate"/>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#endDate">
      <skos:prefLabel xml:lang="en">End date</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#endDate">
      <skos:definition xml:lang="en">End of the validity period. If a resource has an end date then it must be marked as deprecated. (euvoc:XlNotation) End of the validity period. If a resource has an end date then it must be marked as deprecated. (euvoc:XlNote) End of the validity period. If a resource has an end date then it must be marked as deprecated. (skos:Concept) End of the validity period. If a resource has an end date then it must be marked as deprecated. (skos:ConceptScheme) End of the validity period. If a resource has an end date then it must be marked as deprecated. (skosxl:Label)</skos:definition>
   </rdf:Description>
   <owl:DatatypeProperty rdf:about="http://publications.europa.eu/ontology/euvoc#startDate"/>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#startDate">
      <skos:prefLabel xml:lang="en">Start date</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#startDate">
      <skos:definition xml:lang="en">Beginning of the validity period. (euvoc:XlNotation) Beginning of the validity period. (euvoc:XlNote) Beginning of the validity period. (skos:Concept) Beginning of the validity period. (skos:ConceptScheme) Beginning of the validity period. (skosxl:Label)</skos:definition>
   </rdf:Description>
   <owl:DatatypeProperty rdf:about="http://www.w3.org/2002/07/owl#deprecated"/>
   <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#deprecated">
      <skos:prefLabel xml:lang="en">Deprecated</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#deprecated">
      <skos:definition xml:lang="en">States whether the resource is current or deprecated. By deprecating a resource, it means that it should not be used in new documents. Deprecation is a feature commonly used in versioning software to indicate that a particular feature is preserved for backward-compatibility purposes, but may be phased out in the future. (euvoc:XlNotation) States whether the resource is current or deprecated. By deprecating a resource, it means that it should not be used in new documents. Deprecation is a feature commonly used in versioning software to indicate that a particular feature is preserved for backward-compatibility purposes, but may be phased out in the future. (euvoc:XlNote) States whether the resource is current or deprecated. By deprecating a resource, it means that it should not be used in new documents. Deprecation is a feature commonly used in versioning software to indicate that a particular feature is preserved for backward-compatibility purposes, but may be phased out in the future (skos:Concept) States whether the resource is current or deprecated. By deprecating a resource, it means that it should not be used in new documents. Deprecation is a feature commonly used in versioning software to indicate that a particular feature is preserved for backward-compatibility purposes, but may be phased out in the future (skos:ConceptScheme) States whether the resource is current or deprecated. By deprecating a resource, it means that it should not be used in new documents. Deprecation is a feature commonly used in versioning software to indicate that a particular feature is preserved for backward-compatibility purposes, but may be phased out in the future. (skosxl:Label)</skos:definition>
   </rdf:Description>
   <owl:DatatypeProperty rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#value"/>
   <rdf:Description rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#value">
      <skos:prefLabel xml:lang="en">Value</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#value">
      <skos:definition xml:lang="en">The literal form of the note. (euvoc:XlNotation) The literal form of the note. (euvoc:XlNote)</skos:definition>
   </rdf:Description>
   <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/creator"/>
   <rdf:Description rdf:about="http://purl.org/dc/terms/creator">
      <skos:prefLabel xml:lang="en">Creator</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://purl.org/dc/terms/creator">
      <skos:definition xml:lang="en">The entity or entities responsible for creating the resource, (euvoc:XlNote)</skos:definition>
   </rdf:Description>
   <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/dateAccepted"/>
   <rdf:Description rdf:about="http://purl.org/dc/terms/dateAccepted">
      <skos:prefLabel xml:lang="en">Date accepted</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://purl.org/dc/terms/dateAccepted">
      <skos:definition xml:lang="en">Date of acceptance of the resource. (skos:Concept)</skos:definition>
   </rdf:Description>
   <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/dateSubmitted"/>
   <rdf:Description rdf:about="http://purl.org/dc/terms/dateSubmitted">
      <skos:prefLabel xml:lang="en">Date submitted</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://purl.org/dc/terms/dateSubmitted">
      <skos:definition xml:lang="en">Date of submission of the resource. (skos:Concept)</skos:definition>
   </rdf:Description>
   <rdf:Property rdf:about="http://www.w3.org/2002/07/owl#versionInfo"/>
   <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#versionInfo">
      <skos:prefLabel xml:lang="en">Version info</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#versionInfo">
      <skos:definition xml:lang="en">An owl:versionInfo statement generally has as its object a string giving information about this version. This statement does not contribute to the logical meaning of the resource. (skos:Concept) An owl:versionInfo statement generally has as its object a string giving information about this version. This statement does not contribute to the logical meaning of the resource. (skos:ConceptScheme)</skos:definition>
   </rdf:Description>
   <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/identifier"/>
   <rdf:Description rdf:about="http://purl.org/dc/terms/identifier">
      <skos:prefLabel xml:lang="en">Identifier</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://purl.org/dc/terms/identifier">
      <skos:definition xml:lang="en">An unambiguous reference to the resource within a given context. Recommended best practice is to identify the resource by means of a string conforming to a formal identification system. (skos:ConceptScheme)</skos:definition>
   </rdf:Description>
   <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/title"/>
   <rdf:Description rdf:about="http://purl.org/dc/terms/title">
      <skos:prefLabel xml:lang="en">Title</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://purl.org/dc/terms/title">
      <skos:definition xml:lang="en">It represents the title or name of a resource. (skos:ConceptScheme)</skos:definition>
   </rdf:Description>
   <owl:DatatypeProperty rdf:about="http://www.w3.org/2008/05/skos-xl#literalForm"/>
   <rdf:Description rdf:about="http://www.w3.org/2008/05/skos-xl#literalForm">
      <skos:prefLabel xml:lang="en">Literal form</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.w3.org/2008/05/skos-xl#literalForm">
      <skos:definition xml:lang="en">An instance of the class skosxl:Label has one and only one literal form. (skosxl:Label)</skos:definition>
   </rdf:Description>
</rdf:RDF>
