    sh:datatype xsd:dateTime ;
    sh:maxCount 1 ;
    sh:name "Created" ;
    sh:path dcterms:created .

tech-shape:vcard-Address-dct-modified a sh:PropertyShape ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:datatype xsd:dateTime ;
    sh:maxCount 1 ;
    sh:name "Modified" ;
    sh:path dcterms:modified .

tech-shape:vcard-Address-dct-type a sh:PropertyShape ;
    sh:class euvoc:AddressType ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "Type" ;
    sh:path dcterms:type ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://purl.org/dc/terms/type> ?that . ?that <http://purl.org/dc/terms/type> ?this .}" ] .

tech-shape:vcard-Address-euvoc-postbox a sh:PropertyShape ;
