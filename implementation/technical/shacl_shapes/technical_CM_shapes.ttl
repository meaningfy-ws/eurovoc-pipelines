    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:name "Postbox" ;
    sh:path euvoc:postbox .

tech-shape:vcard-Address-vcard-locality a sh:PropertyShape ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "Locality" ;
    sh:path vcard:locality .

tech-shape:vcard-Address-vcard-post-office-box a sh:PropertyShape ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:name "Post-office-box" ;
    sh:path vcard:post-office-box .

tech-shape:vcard-Address-vcard-postal-code a sh:PropertyShape ;
    rdfs:isDefinedBy tech-shape:tech-shape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:name "Postal-code" ;
    sh:path vcard:postal-code .

tech-shape:vcard-Address-vcard-street-address a sh:PropertyShape ;
