   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://publications.europa.eu/ontology/euvoc#legallyBasedOnTreaty"/>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#legallyBasedOnTreaty">
      <skos:prefLabel xml:lang="en">Legally based on treaty</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#legallyBasedOnTreaty">
      <skos:definition xml:lang="en">It indicates that the corporate body's operations, existence, or functions are legally grounded in or regulated by the terms and provisions of the specified treaty. (euvoc:CorporateBody -&gt; euvoc:Treaty (+euvoc:legallyBasedOnTreaty))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://publications.europa.eu/ontology/euvoc#context"/>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#context">
      <skos:prefLabel xml:lang="en">Context</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#context">
      <skos:definition xml:lang="en">It signifies that the concept is applicable or relevant within the context of the identified use context. (skos:Concept -&gt; euvoc:UseContext (+euvoc:context))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://www.w3.org/ns/locn#geometry"/>
   <rdf:Description rdf:about="http://www.w3.org/ns/locn#geometry">
      <skos:prefLabel xml:lang="en">Geometry</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.w3.org/ns/locn#geometry">
      <skos:definition xml:lang="en">It indicates the spatial or geographical representation of each site. (euvoc:Site -&gt; sf:Polygon (+locn:geometry))</skos:definition>
   </rdf:Description>
