   <owl:ObjectProperty rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#gender"/>
   <rdf:Description rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#gender">
      <skos:prefLabel xml:lang="en">Gender</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#gender">
      <skos:definition xml:lang="en">A grammatical category that indicates grammatical relationships between words in sentences. (skosxl:Label -&gt; euvoc:GrammaticalGender (+lexinfo:gender))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#number"/>
   <rdf:Description rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#number">
      <skos:prefLabel xml:lang="en">Number</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#number">
      <skos:definition xml:lang="en">Grammatical category for the variation in form of nouns, pronouns, and any words agreeing with them, depending on how many persons or things are referred to. (skosxl:Label -&gt; euvoc:GrammaticalNumber (+lexinfo:number))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://lexvo.org/ontology#inScript"/>
   <rdf:Description rdf:about="http://lexvo.org/ontology#inScript">
