   </rdf:Description>
   <rdf:Description rdf:about="http://publications.europa.eu/ontology/euvoc#corrStatus">
      <skos:definition xml:lang="en">A status of proofreading of labels and descriptions. (euvoc:XlNote -&gt; euvoc:CorrectionStatus (+euvoc:corrStatus)) A status of proofreading of labels and descriptions. (skosxl:Label -&gt; euvoc:CorrectionStatus (+euvoc:corrStatus))</skos:definition>
   </rdf:Description>
   <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#inScheme"/>
